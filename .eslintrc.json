{
  "env": {
    "browser": true,
    "mocha": true
  },
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "optionalDependencies": false, "peerDependencies": false}],
    "no-unused-expressions": ["error", { "allowTaggedTemplates": true }],
    "react/jsx-filename-extension": 0
  },
  "parser": "babel-eslint"
}
